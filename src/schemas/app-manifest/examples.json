{
  "$schema": "https://reportportal.io/schema-registry/app-manifest/v1.0/schema.json",
  "manifestVersion": "1.0",
  "id": "app-name",
  "name": "App Name",
  "version": "1.0.0",
  "description": "A description of the app",
  "developer": {
    "name": "Publisher Name",
    "websiteUrl": "https://example.com"
  },
  "documentation": "http://example.com/docs",
  "group": "bts",
  "license": "Apache-2.0",
  "icon": {
    "type": "url",
    "content": "/path/to/icon.jpg"
  },
  "baseUrl": "https://example.com",
  "permissions": {
    "scopes": []
  },
  "modules": {
    "projectPages": [
      {
        "slug": "my-project-page",
        "name": "My Project Page",
        "url": "/index",
        "icon": {
          "type": "url",
          "content": "/path/to/icon.png"
        }
      },
      {
        "slug": "my-other-project-page",
        "name": "My Other Project Page",
        "url": "/other.html",
        "icon": {
          "type": "base64",
          "content": "data:image/png;base64,AsdAD="
        }
      },
      {
        "slug": "my-third-project-page",
        "name": "My Third Project Page",
        "url": "/third.html",
        "icon": {
          "type": "svg",
          "content": "<svg>...</svg>"
        }
      }
    ]
  }
}